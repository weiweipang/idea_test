<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!--驼峰命名-->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <!--将动态拼接的sql打印在控制台-->
        <!-- <setting name="logImpl" value="STDOUT_LOGGING" />-->
    </settings>
    <typeAliases>
        <!--该包下所有类都可以用别名的方式访问，不特殊指定的话就是改类的类名小写方式-->
        <package name="cn.ssm.pojo"/>
    </typeAliases>

  <!--  <plugins>
        &lt;!&ndash; com.github.pagehelper为PageHelper类所在包名 &ndash;&gt;
        <plugin interceptor="com.github.pagehelper.PageInterceptor">
            &lt;!&ndash; 设置数据库类型 Oracle,Mysql,MariaDB,SQLite,Hsqldb,PostgreSQL六种数据库 &ndash;&gt;
            <property name="helperDialect" value="mysql"/>
            &lt;!&ndash;分页合理化，大于总页数时显示最后一页&ndash;&gt;
            <property name="reasonable" value="true"/>
        </plugin>
    </plugins>-->
</configuration>